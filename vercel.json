{
  "version": 2,
  "name": "rag-system-001-web",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "buildCommand": "cd apps/web && npm run build",
  "devCommand": "cd apps/web && npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "apps/web/.next",
  "framework": "nextjs",
  "functions": {
    "apps/web/app/api/**": {
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "NODE_VERSION": "20"
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}